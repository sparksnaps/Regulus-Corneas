local p=game.Players.LocalPlayer
local g=p.PlayerGui
local h=g:FindFirstChild("Hotbar")
local b=h:FindFirstChild("Backpack")
local f=b:FindFirstChild("Hotbar")
local names={"Absolute Stillness「絶対静止」","Time-Sever Barrage「時間断裂」","Authority Push「権能圧殺」","Frozen Reality Break「世界停止破壊」"}
for i=1,4 do local slot=f:FindFirstChild(tostring(i)) slot.Base.ToolName.Text=names[i] slot.Base.ToolName.Font=Enum.Font.Fantasy end

local player=p
local character=player.Character or player.CharacterAdded:Wait()
local humanoid=character:WaitForChild("Humanoid")
local root=character:WaitForChild("HumanoidRootPart")

local TextService=game:GetService("TextService")
local TweenService=game:GetService("TweenService")
local Debris=game:GetService("Debris")

local function easeOutQuad(x)
    return 1+2.70158*math.pow(x-1,3)+1.70158*math.pow(x-1,2)
end

local function getColor(t,keypoints)
    local k1,k2,alpha=keypoints[1],keypoints[1],0.5
    for i=1,#keypoints-1 do
        if keypoints[i].Time<=t and t<=keypoints[i+1].Time then
            k1=keypoints[i]
            k2=keypoints[i+1]
            alpha=(t-k1.Time)/(k2.Time-k1.Time)
            return k1.Value:lerp(k2.Value,alpha)
        end
    end
    return k1.Value
end

local function createKJTemplate()
    local sg=Instance.new("ScreenGui")
    sg.Name="KJDialogueTemplate"
    sg.ResetOnSpawn=false
    sg.DisplayOrder=1000
    sg.IgnoreGuiInset=true
    local holder=Instance.new("Frame",sg)
    holder.Name="Holder"
    holder.Size=UDim2.new(1,0,1,0)
    holder.Position=UDim2.new(0.5,0,1,0)
    holder.AnchorPoint=Vector2.new(0.5,1)
    holder.BackgroundTransparency=1
    holder.ClipsDescendants=false
    holder.ZIndex=10
    local template=Instance.new("Frame",holder)
    template.Name="Template"
    template.Size=UDim2.new(1,0,1,0)
    template.BackgroundTransparency=1
    template.ClipsDescendants=false
    template.ZIndex=10
    return sg
end

local function retireTexts(frame)
    for _,v in frame:GetChildren() do
        if v.Name=="letter" then
            v:SetAttribute("Ending",true)
            TweenService:Create(v,TweenInfo.new(0.5,Enum.EasingStyle.Quad,Enum.EasingDirection.In),{Position=v.Position+UDim2.new(0,0,0,50),TextTransparency=1,TextStrokeTransparency=1}):Play()
            Debris:AddItem(v,0.5)
        end
    end
end

local function doText(textData,speakerName)
    local playerGui=player:WaitForChild("PlayerGui")
    local guiName=speakerName.."KJUI"
    local gui=playerGui:FindFirstChild(guiName)
    if not gui then
        gui=createKJTemplate()
        gui.Name=guiName
        gui.Parent=playerGui
    end
    gui.Enabled=true
    local templateFrame=gui.Holder:FindFirstChild("Template")
    local totalWidth,currentX,typeDelay=0,0,0
    for _,textPart in ipairs(textData) do
        local chars=string.split(textPart.Text,"")
        local font=textPart.Bold and Enum.Font.SourceSansBold or textPart.Italic and Enum.Font.SourceSansItalic or Enum.Font.SourceSans
        for _,char in chars do totalWidth=totalWidth+TextService:GetTextSize(char,25,font,Vector2.new(100,100)).X end
    end
    for _,textPart in ipairs(textData) do
        local chars=string.split(textPart.Text,"")
        local font=textPart.Bold and Enum.Font.SourceSansBold or textPart.Italic and Enum.Font.SourceSansItalic or Enum.Font.SourceSans
        for _,char in chars do
            local textSize=TextService:GetTextSize(char,25,font,Vector2.new(100,100))
            local letter=Instance.new("TextLabel")
            letter.AnchorPoint=Vector2.new(0,0.5)
            letter.Position=UDim2.new(0.5,math.floor(currentX-totalWidth/2)-15,0.5,170)
            letter.Size=UDim2.new(0,textSize.X,0,textSize.Y)
            letter.Text=char
            letter.Name="letter"
            letter.Font=font
            letter.TextSize=25
            letter.ZIndex=20
            letter.Parent=gui.Holder.Template
            letter.BackgroundTransparency=1
            letter.TextStrokeColor3=textPart.TextStrokeColor
            letter.TextStrokeTransparency=1
            letter.TextTransparency=1
            local finalX=math.floor(currentX-totalWidth/2)-15
            local startingX=finalX+100
            task.delay(typeDelay,function()
                local startTimeOs=os.clock()
                repeat
                    local progress=math.min((os.clock()-startTimeOs)/0.35,1)
                    local shakeProgress=math.min((os.clock()-startTimeOs)/textPart.Shake.Lifetime,1)
                    local shake=not textPart.Shake.Enabled and UDim2.new(0,0,0,0) or UDim2.new(0,math.random(-textPart.Shake.Intensity,textPart.Shake.Intensity)*(1-shakeProgress),0,math.random(-textPart.Shake.Intensity,textPart.Shake.Intensity)*(1-shakeProgress))
                    local fade=1-easeOutQuad(progress)
                    local currentXPos=startingX+(finalX-startingX)*easeOutQuad(progress)
                    if letter and letter.Parent then
                        letter.TextStrokeTransparency=(1-progress)^10
                        letter.TextTransparency=fade
                        letter.TextSize=25+25*fade
                        letter.TextColor3=getColor(progress,textPart.Color.Keypoints)
                        letter.Position=UDim2.new(0.5,currentXPos,0.5,170)+shake
                    end
                    task.wait()
                until os.clock()-startTimeOs>math.max(0.35,textPart.Shake.Lifetime) or not letter or not letter:IsDescendantOf(gui) or letter:GetAttribute("Ending")
                if letter and letter.Parent then
                    letter.TextStrokeTransparency=0
                    letter.TextTransparency=0
                    letter.TextSize=25
                    letter.TextColor3=textPart.Color.Keypoints[#textPart.Color.Keypoints].Value
                    letter.Position=UDim2.new(0.5,finalX,0.5,170)
                end
            end)
            typeDelay=typeDelay+textPart.TypeSpeed
            currentX=currentX+textSize.X+1
        end
    end
    task.spawn(function()
        task.wait(5)
        if gui and gui.Parent then
            retireTexts(gui.Holder.Template)
            task.wait(1)
            gui:Destroy()
        end
    end)
end

local function stopAllAnims()
    for _,t in pairs(humanoid:GetPlayingAnimationTracks()) do
        t:Stop()
    end
end

local function playAnimTrack(animId,speed,startTime)
    stopAllAnims()
    local a=Instance.new("Animation")
    a.AnimationId=animId
    local track=humanoid:LoadAnimation(a)
    track:Play()
    track:AdjustSpeed(speed)
    track.TimePosition=startTime
    track:AdjustSpeed(speed)
    return track
end

local function makeVFX()
    local part=Instance.new("Part")
    part.Name="BehindFX"
    part.Size=Vector3.new(1,1,1)
    part.Transparency=1
    part.Anchored=true
    part.CanCollide=false
    part.Parent=workspace
    local offset=CFrame.new(0,0,2)
    local conn=game:GetService("RunService").Heartbeat:Connect(function()
        if not character.Parent or not root.Parent then
            conn:Disconnect()
            if part.Parent then part:Destroy() end
            return
        end
        part.CFrame=root.CFrame*offset
    end)
    
    local ptc1=Instance.new("ParticleEmitter")
    ptc1.Color=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(150,144,149)),ColorSequenceKeypoint.new(1,Color3.fromRGB(150,144,149))})
    ptc1.Drag=6
    ptc1.LightInfluence=0
    ptc1.Speed=NumberRange.new(8,70)
    ptc1.Size=NumberSequence.new({NumberSequenceKeypoint.new(0,4,0),NumberSequenceKeypoint.new(1,4,0)})
    ptc1.RotSpeed=NumberRange.new(-120,120)
    ptc1.Texture='rbxassetid://9544354287'
    ptc1.EmissionDirection=Enum.NormalId.Back
    ptc1.Transparency=NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.09,0.59,0),NumberSequenceKeypoint.new(0.25,0.76,0),NumberSequenceKeypoint.new(0.67,0.93,0),NumberSequenceKeypoint.new(1,1,0)})
    ptc1.SpreadAngle=Vector2.new(10,30)
    ptc1.Rotation=NumberRange.new(-360,360)
    ptc1.Lifetime=NumberRange.new(0.45,1.4)
    ptc1.Parent=part
    
    local ptc2=Instance.new("ParticleEmitter")
    ptc2.Color=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(150,144,149)),ColorSequenceKeypoint.new(1,Color3.fromRGB(150,144,149))})
    ptc2.Drag=6
    ptc2.ZOffset=1
    ptc2.LightInfluence=0
    ptc2.Acceleration=Vector3.new(0,-15,0)
    ptc2.Speed=NumberRange.new(8,70)
    ptc2.Size=NumberSequence.new({NumberSequenceKeypoint.new(0,4,0),NumberSequenceKeypoint.new(1,4,0)})
    ptc2.RotSpeed=NumberRange.new(-120,120)
    ptc2.Texture='rbxassetid://7216854921'
    ptc2.EmissionDirection=Enum.NormalId.Back
    ptc2.Transparency=NumberSequence.new({NumberSequenceKeypoint.new(0,0.47,0),NumberSequenceKeypoint.new(0.25,0.76,0),NumberSequenceKeypoint.new(0.67,0.93,0),NumberSequenceKeypoint.new(1,1,0)})
    ptc2.SpreadAngle=Vector2.new(25,30)
    ptc2.Rotation=NumberRange.new(-360,360)
    ptc2.LightEmission=1
    ptc2.Lifetime=NumberRange.new(0.45,1.4)
    ptc2.Parent=part
    
    local ptc3=Instance.new("ParticleEmitter")
    ptc3.Color=ColorSequence.new({ColorSequenceKeypoint.new(0,Color3.fromRGB(150,144,149)),ColorSequenceKeypoint.new(1,Color3.fromRGB(150,144,149))})
    ptc3.Drag=6
    ptc3.ZOffset=1
    ptc3.LightInfluence=0
    ptc3.Squash=NumberSequence.new({NumberSequenceKeypoint.new(0,0,0),NumberSequenceKeypoint.new(1,3,0)})
    ptc3.Speed=NumberRange.new(12,102)
    ptc3.Size=NumberSequence.new({NumberSequenceKeypoint.new(0,2,0),NumberSequenceKeypoint.new(1,2,0)})
    ptc3.Texture='rbxassetid://9544366974'
    ptc3.EmissionDirection=Enum.NormalId.Back
    ptc3.Transparency=NumberSequence.new({NumberSequenceKeypoint.new(0,0.77,0),NumberSequenceKeypoint.new(0.46,0.92,0),NumberSequenceKeypoint.new(1,1,0)})
    ptc3.SpreadAngle=Vector2.new(0,30)
    ptc3.Rotation=NumberRange.new(-90,-90)
    ptc3.LightEmission=1
    ptc3.Lifetime=NumberRange.new(0.45,1.4)
    ptc3.Orientation=Enum.ParticleOrientation.VelocityParallel
    ptc3.Parent=part
    
    task.delay(1,function()
        if conn then conn:Disconnect() end
        if part.Parent then part:Destroy() end
    end)
end

