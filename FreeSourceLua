local StopIds={[17859015788]=true,[10469493270]=true,[10469630950]=true,[10469639222]=true,[10469643643]=true}
local Replacements={
    ["10469493270"]={id="rbxassetid://17325513870",speed=1.5,startTime=0,sfx="rbxassetid://81022009064023",sfxStartTime=0},
    ["10469630950"]={id="rbxassetid://17325510002",speed=1.5,startTime=0.2,sfx="rbxassetid://99595080052889",sfxStartTime=0},
    ["10469639222"]={id="rbxassetid://17325513870",speed=1.5,startTime=0.1,sfx="rbxassetid://122359705789431",sfxStartTime=0},
    ["10469643643"]={id="rbxassetid://18169291044",speed=1.5,startTime=0,sfx="rbxassetid://83947221567929",sfxStartTime=0},
